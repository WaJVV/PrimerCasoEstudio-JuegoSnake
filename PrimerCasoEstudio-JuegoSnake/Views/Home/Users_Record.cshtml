@{
}
<body>
    <h1>Historial de Partidas</h1>

    <table class="table">
        <thead>
            <tr>
                <th>ID</th>
                <th>User Name</th>
                <th>Tiempo</th>
                <th>Puntaje</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var user in Model)
            {
                <tr>
                    <td>@user.id</td>
                    <td>@user.user</td>
                    <td>@user.time</td>
                    <td>@user.score</td>
                </tr>
            }
        </tbody>
    </table>

    <button onclick="gotoMain()" class="start">Volver a Jugar</button>
</body>